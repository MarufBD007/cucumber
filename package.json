{
  "name": "testing",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run test:api; npm run test:web;",
    "test:api": "npm run cucumber:api; npm run report:api;",
    "test:web": "npm run cucumber:web; npm run report:web;",
    "cucumber:api": "cucumber-js --tags @PositiveTesting ./test/api/features -r ./test/api/steps -f json:test/api/report/cucumber_report.json",
    "cucumber:web": "cucumber-js ./test/web/features -r ./test/web/steps -f json:test/web/report/cucumber_report.json",
    "report:api": "node test/api/index.js",
    "report:web": "node test/web/index.js"
  },
  "author": "Md Maruf Rahman",
  "license": "ISC",
  "devDependencies": {
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.1.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chromedriver": "^79.0.2",
    "dotenv": "^8.2.0",
    "geckodriver": "^1.19.1",
    "selenium-webdriver": "^3.5.0",
    "sleep": "^6.1.0"
  }
}
